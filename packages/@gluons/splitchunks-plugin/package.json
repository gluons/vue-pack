{
  "name": "@gluons/vue-pack-splitchunks-plugin",
  "version": "0.0.1",
  "description": "A vue-pack plugin to customize webpack's splitChunks.",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
