    "@gluons/vue-pack": "^0.0.1"
  },
  "devDependencies": {
    "@gluons/vue-pack": "^0.0.1",
    "@types/jest": "^23.1.0",
    "@types/webpack": "^4.4.0",
    "jest": "^23.1.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
