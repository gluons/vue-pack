  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/vue-pack/issues"
  },
  "homepage": "https://github.com/gluons/vue-pack/tree/master/packages/@gluons/plugin-splitchunks",
  "peerDependencies": {
    "@gluons/vue-pack": "*"
  },
  "devDependencies": {
    "@gluons/vue-pack": "^1.6.0",
    "webpack": "^4.17.3"
  }
