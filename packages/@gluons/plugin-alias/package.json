{
  "name": "@gluons/vue-pack-alias-plugin",
  "version": "1.0.1",
  "description": "A vue-pack plugin to customize webpack's resolve.alias.",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf dist/*",
    "build": "tsc -p .",
    "lint": "tslint -p .",
    "pretest": "npm run build",
    "test": "npm run lint && jest -c ./jest.config.json --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/vue-pack.git"
  },
  "keywords": [
    "vue-pack",
    "vue-pack-plugin",
    "webpack",
    "resolve",
    "alias",
    "resolve.alias"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/vue-pack/issues"
  },
  "homepage": "https://github.com/gluons/vue-pack/tree/master/packages/@gluons/plugin-alias",
  "peerDependencies": {
    "@gluons/vue-pack": "*"
  },
  "devDependencies": {
    "@gluons/vue-pack": "^1.0.0",
    "jest": "^23.4.2",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.0"
  }
}
