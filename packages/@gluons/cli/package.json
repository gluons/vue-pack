{
  "name": "@gluons/vue-pack-cli",
  "version": "1.17.0",
  "description": "A CLI tool for vue-pack.",
  "bin": {
    "vue-pack": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "lint": "tslint -p .",
    "pretest": "npm run build && ts-node ./scripts/prepareTest.ts",
    "test": "npm run lint && testcafe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/vue-pack.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "library",
    "bundler",
    "packager",
    "webpack",
    "vue-pack",
    "cli"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/vue-pack/issues"
  },
  "homepage": "https://gluons.github.io/vue-pack/",
  "peerDependencies": {
    "vue": "2"
  },
  "dependencies": {
    "@gluons/vue-pack": "^1.17.0",
    "@gluons/vue-pack-dev": "^1.17.0",
    "@gluons/vue-pack-load-config": "^1.17.0",
    "@gluons/vue-pack-types": "^1.17.0",
    "aggregate-error": "^2.1.0",
    "chalk": "^2.4.2",
    "lodash.isnil": "^4.0.0",
    "lodash.omitby": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "nvl": "^1.2.2",
    "yargs": "^13.2.1"
  },
  "devDependencies": {
    "@types/aggregate-error": "^1.0.0",
    "@types/cross-spawn": "^6.0.0",
    "@types/lodash.isnil": "^4.0.5",
    "@types/lodash.omitby": "^4.6.5",
    "@types/lodash.pick": "^4.4.5",
    "@types/yargs": "^12.0.9",
    "cross-spawn": "^6.0.5"
  }
}
